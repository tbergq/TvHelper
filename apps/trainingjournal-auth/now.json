{
  "version": 2,
  "name": "trainingjournal-auth",
  "builds": [
    { "src": "apps/trainingjournal-auth/src/swagger-ui-dist/swagger-ui.css", "use": "@now/static" },
    { "src": "apps/trainingjournal-auth/src/swagger-ui-dist/swagger-ui-bundle.js", "use": "@now/static" },
    { "src": "apps/trainingjournal-auth/src/swagger-ui-dist/swagger-ui-standalone-preset.js", "use": "@now/static" },
    { "src": "apps/trainingjournal-auth/src/index.js", "use": "@now/node", "config": { "maxLambdaSize": "50mb" } }
  ],
  "env": {
    "NODE_ENV": "production",
    "TJA_DB_URL":"@tja_db_url"
  },
  "alias": "trainingjournal-auth.now.sh",
  "routes": [
    {
      "src": "/swagger/swagger-ui.css",
      "dest": "/apps/trainingjournal-auth/src/swagger-ui-dist/swagger-ui.css"
    },
    {
      "src": "/swagger/swagger-ui-bundle.js",
      "dest": "/apps/trainingjournal-auth/src/swagger-ui-dist/swagger-ui-bundle.js"
    },
    {
      "src": "/swagger/swagger-ui-standalone-preset.js",
      "dest": "/apps/trainingjournal-auth/src/swagger-ui-dist/swagger-ui-standalone-preset.js"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/trainingjournal-auth/src"
    }
  ]
}
