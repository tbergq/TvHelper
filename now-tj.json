{
  "version": 2,
  "name": "trainingjournal",
  "builds": [
    {
      "src": "apps/trainingjournal/next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "alias": "trainingjournal.now.sh",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/trainingjournal/$1"
    }
  ]
}